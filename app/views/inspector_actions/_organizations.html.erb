<%= paginate @organizations %>

<div>
  <% @organizations.each do |organization| %>
    <div>
      <p>
        <%= link_to organization.name, organization_path(organization.id) %>
      </p>
      <p>
        <%= organization.org_inn %>
      </p>
      <p>
        <% user = User.find(organization.user_id) %>
        Руководитель: <%= link_to user.full_name, show_user_path(user.id) %>
      </p>
    </div>
  <% end %>
</div>

<%= paginate @organizations %>