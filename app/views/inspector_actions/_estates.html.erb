<%= paginate @estates %>

<div>
  <% @estates.each do |estate| %>
    <div>
      <p>
        <%= link_to estate.address, estate_path(estate.id) %>
      </p>
      <p>
        <%= estate.cadastral_number %>
      </p>
      <p>
        <% user = User.find(estate.user_id) %>
        <%= link_to user.full_name, show_user_path(user.id) %>
      </p>
    </div>
  <% end %>
</div>

<%= paginate @estates %>