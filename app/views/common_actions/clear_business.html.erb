<%= form_with(url: find_business_path, method: :get, local: true) do |form| %>
  <div>
    <%= form.label :identificator, "ИНН индивидуального предпринимателя или юридического лица", style: "display: block" %>
    <%= form.text_field :identificator %>
  </div>

  <div>
    <%= form.submit "Продолжить", class: "btn btn-primary" %>
  </div>
<% end %>
<div id="result">
</div>